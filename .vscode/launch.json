    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch decision-helper app in debug",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${fileDirname}/../webapp/server/main.go",
            "env": {
                "APPLICATION_CONFIG_PATH":"./../application.yaml",
                "DATASOURCE_URL": "postgresql://decision_helper:decision_helper@localhost:5432/decision_helper?application_name=decision-helper&sslmode=disable"
              }
        }
    ]
}